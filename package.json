{
  "name": "vue-a-color-picker",
  "version": "0.0.2",
  "description": "a-color-picker wrapper for Vue.js",
  "main": "dist/AColorPicker.umd.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:lib": "vue-cli-service build --target lib --name AColorPicker src/AColorPicker.vue",
    "lint": "vue-cli-service lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/narsenico/vue-a-color-picker.git"
  },
  "keywords": [
    "color",
    "picker",
    "vue",
    "javascript"
  ],
  "author": "narsenico",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/narsenico/vue-a-color-picker/issues"
  },
  "homepage": "https://github.com/narsenico/vue-a-color-picker#readme",
  "dependencies": {},
  "peerDependencies": {
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.4",
    "a-color-picker": "^1.1.7",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "vue": {
    "css": {
      "extract": false
    }
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
